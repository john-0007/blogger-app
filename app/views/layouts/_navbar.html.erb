<div class="ui fixed inverted menu">
  <div class="ui container">
    <div class="header item">
      <i class="code icon large"></i>
      blogger</div>
    <%= link_to "Home", blogs_path, class: "item"%>
    <%= link_to "New Blog", new_blog_path, class: "item"%>
    <div class="right menu">
      <% if current_user %>
      <div class="item">
        <%= current_user.username %>
      </div>
      <div class="item">
        <%= link_to "Log out", session_path,method: :delete%>
      </div>

      <%else%>
      <%= link_to "Sing in", new_session_path, class: "item"%>
      <!-- <a href="/singup" class="item">Sign Up</a> -->
      <%= link_to "Sing Up", new_user_path, class:"item" %>
      <%end%>

    </div>
  </div>
</div>
